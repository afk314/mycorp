{
  "template":{
    "description":"HW XML Template",
    "context":"hw.doc",
    "pathNamespace":[
      {
        "prefix":"concept_schema",
        "namespaceUri":"http://metadata.healthwise.org/concept/concept_schema"
      },
      {
        "prefix":"content_asset_schema",
        "namespaceUri":"http://metadata.healthwise.org/content/content_asset_schema"
      },
      {
        "prefix":"rdf",
        "namespaceUri":"http://www.w3.org/1999/02/22-rdf-syntax-ns#>"
      },
      {
        "prefix":"rdfs",
        "namespaceUri":"http://www.w3.org/2000/01/rdf-schema#"
      },
      {
        "prefix":"skos",
        "namespaceUri":"http://www.w3.org/2004/02/skos/core#"
      },
      {
        "prefix":"dct",
        "namespaceUri":"http://purl.org/dc/terms/"
      },
      {
        "prefix":"content_asset_data",
        "namespaceUri":"http://metadata.healthwise.org/content/content_asset_data"
      }
    ],
    "collections":[
      "hwxml"
    ],
    "vars":[
      {
        "name":"id",
        "val":"./@hwid-content"
      }
    ],
    "triples":[
      {
        "subject":{
          "val":"sem:iri(concat($content_asset_data,$id))",
          "invalidValues":"ignore"
        },
        "predicate":{
          "val":"sem:iri(concat($content_asset_schema,'asset_id))"
        },
        "object":{
          "val":"$id"
        }
      }
    ]
  }
}
